      "args": [
        "--quadtree",
        "4",
        "--quadshow",
        "True"
      ]
    },
    {
      "name": "Python: KdTree",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "--filename",
        "./assignment3/IS-GIS-Lab/data/addressen_groningen/adressen_groningen.shp",
        "--bbox-depth",
        "8",
        "--max-depth",
        "9",
        "--plot",
        "kdtree-bb",
